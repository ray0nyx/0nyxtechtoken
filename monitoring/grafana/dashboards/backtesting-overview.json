{
  "dashboard": {
    "id": null,
    "title": "Backtesting Platform Overview",
    "tags": ["backtesting", "platform", "overview"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"lean-service\"}",
            "legendFormat": "LEAN Service"
          },
          {
            "expr": "up{job=\"data-service\"}",
            "legendFormat": "Data Service"
          },
          {
            "expr": "up{job=\"websocket-service\"}",
            "legendFormat": "WebSocket Service"
          },
          {
            "expr": "up{job=\"job-workers\"}",
            "legendFormat": "Job Workers"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Active Backtests",
        "type": "stat",
        "targets": [
          {
            "expr": "backtest_requests_total{status=\"running\"}",
            "legendFormat": "Running"
          },
          {
            "expr": "backtest_requests_total{status=\"completed\"}",
            "legendFormat": "Completed"
          },
          {
            "expr": "backtest_requests_total{status=\"failed\"}",
            "legendFormat": "Failed"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Backtest Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(backtest_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(backtest_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(backtest_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "redis_queue_length",
            "legendFormat": "Queue Length"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Worker Utilization",
        "type": "graph",
        "targets": [
          {
            "expr": "active_containers_total",
            "legendFormat": "Active Containers"
          },
          {
            "expr": "container_cpu_usage_percent",
            "legendFormat": "CPU Usage %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "container_memory_usage_bytes",
            "legendFormat": "Memory Usage"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(backtest_requests_total{status=\"failed\"}[5m]) / rate(backtest_requests_total[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
